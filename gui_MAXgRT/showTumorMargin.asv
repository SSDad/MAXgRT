function showTumorMargin(hPlotObj, cineData, iSlice)

global hFig
data = guidata(hFig

% Tumor
ov = cineData.Tumor.OL;

if iSlice <= size(ov, 3)
    bw = ov(:,:,iSlice);
    
    ovA = hPlotObj.TumorOLView.CData(:, :, 1);
    ovA(bw) = 0;
    hPlotObj.TumorOLView.CData(:, :, 1) = ovA;
    
end